<?xml version="1.0"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
    <appSettings>
        <add key="StartMessagePattern" value="^\d{4}-\d{2}-\d{2} \d{2}:\d{2}:\d{2},\d{3}.*" />
        <add key="TraceMarker" value="TRACE" />
        <add key="DebugMarker" value="DEBUG" />
        <add key="InfoMarker" value="INFO" />
        <add key="WarnMarker" value="WARN" />
        <add key="ErrorMarker" value="ERROR" />
        <add key="FatalMarker" value="FATAL" />
    </appSettings>
</configuration>
