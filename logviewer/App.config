<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
        <add key="LOGENTRIES_TOKEN" value="1412eccc-1544-484c-9fbe-9deff564a360"/>
        <add key="StartMessagePattern" value="^\[?\d{4}-\d{2}-\d{2}\s+\d{2}:\d{2}:\d{2}([,.]\d{3,})?.*" />
        <add key="TraceMarker" value="TRACE|\[[Tt]race\]" />
        <add key="DebugMarker" value="DEBUG|\[[Dd]ebug\]" />
        <add key="InfoMarker" value="INFO|\[[Ii]nfo\]" />
        <add key="WarnMarker" value="WARN|\[[Ww]arn\]" />
        <add key="ErrorMarker" value="ERROR|\[[Ee]rror\]" />
        <add key="FatalMarker" value="FATAL|\[[Ff]atal\]" />
        <add key="SettingsDatabase" value="LogviewerSettings.db" />
    </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Data.SQLite" publicKeyToken="db937bc2d44ff139" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.0.93.0" newVersion="1.0.93.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
